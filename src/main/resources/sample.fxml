<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="91.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.levinwinter.Controller">
    <AnchorPane prefHeight="121.0" prefWidth="451.0" VBox.vgrow="ALWAYS">
        <TextField fx:id="textField" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="25.0"
                   prefWidth="282.0"/>
        <Button layoutX="304.0" layoutY="14.0" mnemonicParsing="false"
                onAction="#selectFolderClicked" prefHeight="25.0" prefWidth="135.0" text="select root directory"/>
        <Button layoutX="304.0" layoutY="52.0" mnemonicParsing="false" onAction="#goClicked" prefHeight="25.0"
                prefWidth="135.0" text="build presentation"/>
        <ProgressIndicator layoutX="447.0" layoutY="28.0" progress="0.0"/>
    </AnchorPane>
</VBox>
